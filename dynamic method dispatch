import java.io.*;
import java.util.*;
class A 
{
    void callme()
    {
        System.out.println("i am from class A");
    }
}
class B extends A
{
    void call_me()
    {
        System.out.println("i am from class B");
    }
}
class Main{
    public static void main(String args[]){
        A a=new A();
         a.callme();
         B b=new B();
         b.callme();
         A a1=new B();
         a1.callme();
    }
}
