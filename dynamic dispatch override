import java.io.*;
import java.util.*;
class Bike{
    void run(){
        System.out.println("Bike is running");
    }
}
class Main extends Bike{
    void run(){
        System.out.println("Splender is running with 60 kmph");
    }
    public static void main(String args[]){
        Main s=new Main();
        s.run();
        Bike b1=new Bike();
        b1.run();
        Bike b2=new Main();
        b2.run();
    }
}
